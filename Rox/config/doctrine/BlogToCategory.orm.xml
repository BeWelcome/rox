<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\BlogToCategory" table="blog_to_category">
    <indexes>
      <index name="blog_category_id_foreign" columns="blog_category_id_foreign"/>
      <index name="blog_id_foreign" columns="blog_id_foreign"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="blogCategoryIdForeign" type="integer" column="blog_category_id_foreign" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="blogIdForeign" type="integer" column="blog_id_foreign" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
