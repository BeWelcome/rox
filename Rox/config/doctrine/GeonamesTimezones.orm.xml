<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\GeonamesTimezones" table="geonames_timezones">
    <id name="timezoneid" type="integer" column="TimeZoneId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="offsetjanuary" type="decimal" column="OffsetJanuary" precision="10" scale="0" nullable="false"/>
    <field name="offsetjuly" type="decimal" column="OffsetJuly" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
