<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Guestsonline" table="guestsonline">
    <indexes>
      <index name="updated" columns="updated"/>
    </indexes>
    <id name="ipguest" type="integer" column="IpGuest">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="comment">last time the member  was active</option>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="appearance" type="string" column="appearance" length="32" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">What to show about the member this can be a html thing</option>
      </options>
    </field>
    <field name="lastactivity" type="string" column="lastactivity" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">Last url the member call</option>
      </options>
    </field>
    <field name="status" type="string" column="Status" length="32" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">a copy of the status of the member (sometime he can be ActiveHidden)</option>
        <option name="default">Active</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
