{
    "name": "bewelcome/rox",
    "description": "The source code that drives bewelcome.org",
    "homepage": "https://www.bewelcome.org/",
    "name": "raymund/project",
    "license": "proprietary",
    "type": "project",
    "license": "GPL",
    "require": {
        "php": ">=5.4.0",
        "anthonymartin/geo-location": "^1.0",
        "ezyang/htmlpurifier": "^4.0",
        "foolz/sphinxql-query-builder": "^1.0",
        "html2text/html2text": "^4.0",
        "michelf/php-markdown": "*",
        "nesbot/carbon": "^1.21",
        "psr/log": "*",
        "robmorgan/phinx": "*",
        "swiftmailer/swiftmailer": "^5.0",
        "symfony/symfony": "3.3.5",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "Twig/Twig": "^1.0",
        "twig/extensions": "^1.0",
        "vlucas/phpdotenv": "^2.2",
        "zendframework/zend-uri": "^2.4",
        "dzango/twig-truncate-extension": "^1.0",
        "judev/php-htmltruncator": "^1.1",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "white-october/pagerfanta-bundle": "^1.0",
        "symfony/assetic-bundle": "^2.8",
        "leafo/scssphp": "^0.6.6",
        "egeloen/ckeditor-bundle": "^5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "fzaninotto/faker": "^1.6",
        "phpmetrics/phpmetrics": "^1.8",
        "phploc/phploc": "^3.0",
        "phpmd/phpmd": "^2.4",
        "phpunit/phpunit": "^5.0",
        "sebastian/phpcpd": "dev-master",
        "squizlabs/php_codesniffer": "^2.5",
        "theseer/phpdox": "^0.9",
        "symfony/phpunit-bridge": "^3.0",
        "sensio/generator-bundle": "^3.0",
        "symfony/debug-bundle": "^3.1",
        "symfony/web-profiler-bundle": "^3.1",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "mockery/mockery": "^0.9.6",
        "nelmio/alice": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "Rox\\": "Rox/",
            "Rox\\Activity\\": "module/Activity/src",
            "Rox\\Admin\\": "module/Admin/src",
            "Rox\\Auth\\": "module/Auth/src",
            "Rox\\Core\\": "module/Core/src",
            "Rox\\Forum\\": "module/Forum/src",
            "Rox\\Geo\\": "module/Geo/src",
            "Rox\\I18n\\": "module/I18n/src",
            "Rox\\Member\\": "module/Member/src",
            "Rox\\Message\\": "module/Message/src",
            "Rox\\Start\\": "module/Start/src",
            "Rox\\Trip\\": "module/Trip/src",
            "Rox\\CommunityNews\\": "module/CommunityNews/src"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php",
            "modules/",
            "lib/",
            "tools/",
            "pthacks/",
            "roxlauncher/",
            "build/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
