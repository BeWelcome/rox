{
    "name": "bewelcome/rox",
    "description": "The source code that drives bewelcome.org",
    "homepage": "https://www.bewelcome.org/",
    "type": "project",
    "license": "GPL",
    "minimum-stability": "beta",
    "require": {
        "php": ">=7.1.7",
        "ext-pdo": "*",
        "Twig/Twig": "^2.4",
        "anthonymartin/geo-location": "^1.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/orm": "^2.6",
        "ezyang/htmlpurifier": "^4.0",
        "foolz/sphinxql-query-builder": "^1.0",
        "html2text/html2text": "^4.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "judev/php-htmltruncator": "^1.1",
        "leafo/scssphp": "^0.6.6",
        "michelf/php-markdown": "*",
        "nesbot/carbon": "^1.21",
        "pagerfanta/pagerfanta": "^2.0",
        "psr/log": "*",
        "robmorgan/phinx": "*",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/symfony": "^3.3",
        "twig/extensions": "1.5.1",
        "vlucas/phpdotenv": "^2.2",
        "white-october/pagerfanta-bundle": "^1.0",
        "zendframework/zend-uri": "^2.4"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.0",
        "fzaninotto/faker": "^1.6",
        "hautelook/alice-bundle": "^2.0",
        "mockery/mockery": "^0.9.6",
        "nelmio/alice": "^3.0",
        "phploc/phploc": "^3.0",
        "phpmd/phpmd": "^2.6@dev",
        "phpmetrics/phpmetrics": "^2.0",
        "phpunit/phpunit": "^7.0",
        "sebastian/phpcpd": "^4.0",
        "sensio/generator-bundle": "^3.0",
        "squizlabs/php_codesniffer": "^2.5",
        "symfony/debug-bundle": "^3.1",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/web-profiler-bundle": "^3.1",
        "theseer/phpdox": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
            "Rox\\": "Rox/",
            "Mike42\\": "src/Mike42"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php",
            "modules/",
            "lib/",
            "tools/",
            "pthacks/",
            "roxlauncher/",
            "build/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
