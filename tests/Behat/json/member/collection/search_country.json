{
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "@context": {
            "pattern": "^/api/contexts/Member$",
            "required": true
        },
        "@id": {
            "pattern": "^/api/members$",
            "required": true
        },
        "@type": {
            "pattern": "^hydra:Collection$",
            "required": true
        },
        "hydra:member": {
            "type": "array",
            "additionalItems": false,
            "maxItems": 5,
            "minItems": 5,
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "@id": {
                        "pattern": "^/api/members/[A-Za-z][A-Za-z0-9-.]{2,18}[A-Za-z0-9]$",
                        "required": true
                    },
                    "@type": {
                        "pattern": "^Member$",
                        "required": true
                    },
                    "username": {
                        "type": "string",
                        "required": true
                    },
                    "city": {
                        "type": "object",
                        "required": true,
                        "additionalProperties": false,
                        "properties": {
                            "@type": {
                                "pattern": "^Location$",
                                "required": false
                            },
                            "@id": {
                                "required": false
                            },
                            "name": {
                                "required": true
                            },
                            "latitude": {
                                "required": true
                            },
                            "longitude": {
                                "required": true
                            },
                            "geonameId": {
                                "required": true
                            },
                            "country": {
                                "type": "object",
                                "required": true,
                                "additionalProperties": false,
                                "properties": {
                                    "@type": {
                                        "pattern": "^Country$",
                                        "required": false
                                    },
                                    "@id": {
                                        "required": false
                                    },
                                    "geonameId": {
                                        "required": true
                                    },
                                    "name": {
                                        "required": true
                                    },
                                    "continent": {
                                        "required": true
                                    }
                                }
                            }
                        }
                    },
                    "firstName": {
                        "type": "string",
                        "required": true
                    },
                    "secondName": {
                        "type": [
                            "string",
                            "null"
                        ],
                        "required": true
                    },
                    "lastName": {
                        "type": "string",
                        "required": true
                    },
                    "accommodation": {
                        "type": "string",
                        "required": true
                    },
                    "additionalAccommodationInfo": {
                        "type": "string",
                        "required": false
                    },
                    "ilivewith": {
                        "type": "string",
                        "required": false
                    },
                    "informationtoguest": {
                        "type": "string",
                        "required": false
                    },
                    "typicoffer": {
                        "type": "string",
                        "required": false
                    },
                    "offer": {
                        "type": "string",
                        "required": false
                    },
                    "maxguest": {
                        "type": "integer",
                        "required": false
                    },
                    "maxlenghtofstay": {
                        "type": "string",
                        "required": false
                    },
                    "organizations": {
                        "type": "string",
                        "required": false
                    },
                    "restrictions": {
                        "type": "string",
                        "required": false
                    },
                    "otherrestrictions": {
                        "type": "string",
                        "required": false
                    },
                    "profileSummary": {
                        "type": "string",
                        "required": false
                    },
                    "occupation": {
                        "type": "string",
                        "required": false
                    },
                    "counterguests": {
                        "type": "integer",
                        "required": false
                    },
                    "counterhosts": {
                        "type": "integer",
                        "required": false
                    },
                    "countertrusts": {
                        "type": "integer",
                        "required": false
                    },
                    "gender": {
                        "type": "string",
                        "required": false
                    },
                    "genderofguest": {
                        "type": "string",
                        "required": false
                    },
                    "motivationforhospitality": {
                        "type": "string",
                        "required": false
                    },
                    "website": {
                        "type": "string",
                        "required": false
                    },
                    "hobbies": {
                        "type": "string",
                        "required": false
                    },
                    "books": {
                        "type": "string",
                        "required": false
                    },
                    "music": {
                        "type": "string",
                        "required": false
                    },
                    "movies": {
                        "type": "string",
                        "required": false
                    },
                    "pasttrips": {
                        "type": "string",
                        "required": false
                    },
                    "plannedtrips": {
                        "type": "string",
                        "required": false
                    },
                    "pleasebring": {
                        "type": "string",
                        "required": false
                    },
                    "offerguests": {
                        "type": "string",
                        "required": false
                    },
                    "offerhosts": {
                        "type": "string",
                        "required": false
                    },
                    "publictransport": {
                        "type": "string",
                        "required": false
                    },
                    "hostingInterest": {
                        "type": [
                            "integer",
                            "null"
                        ],
                        "required": false
                    },
                    "lastLogin": {
                        "type": "string",
                        "required": false
                    },
                    "created": {
                        "type": "string",
                        "required": false
                    },
                    "languageLevels": {
                        "type": "array",
                        "required": true,
                        "items": {
                            "@type": {
                                "pattern": "^MembersLanguagesLevel$",
                                "required": false
                            },
                            "@id": {
                                "required": false
                            },
                            "level": {
                                "required": true
                            },
                            "language": {
                                "type": "object",
                                "required": true,
                                "additionalProperties": false,
                                "properties": {
                                    "@type": {
                                        "pattern": "^Language$",
                                        "required": false
                                    },
                                    "@id": {
                                        "required": false
                                    },
                                    "englishname": {
                                        "required": true
                                    },
                                    "shortcode": {
                                        "required": true
                                    }
                                }
                            }
                        }
                    },
                    "memberFields": {
                        "type": [
                            "object",
                            "array"
                        ],
                        "required": true,
                        "additionalProperties": false,
                        "properties": {
                            "en": {
                                "type": "object",
                                "required": false,
                                "additionalProperties": false,
                                "properties": {
                                    "ProfileSummary": {
                                        "required": true
                                    },
                                    "MaxLenghtOfStay": {
                                        "required": true
                                    },
                                    "Occupation": {
                                        "required": true
                                    },
                                    "ILiveWith": {
                                        "required": true
                                    },
                                    "AdditionalAccomodationInfo": {
                                        "required": true
                                    },
                                    "OtherRestrictions": {
                                        "required": true
                                    },
                                    "Hobbies": {
                                        "required": true
                                    },
                                    "Books": {
                                        "required": true
                                    },
                                    "Music": {
                                        "required": true
                                    },
                                    "Movies": {
                                        "required": true
                                    },
                                    "PleaseBring": {
                                        "required": true
                                    },
                                    "OfferGuests": {
                                        "required": true
                                    },
                                    "OfferHosts": {
                                        "required": true
                                    },
                                    "PublicTransport": {
                                        "required": true
                                    },
                                    "MotivationForHospitality": {
                                        "required": true
                                    },
                                    "Offer": {
                                        "required": true
                                    },
                                    "Organizations": {
                                        "required": true
                                    },
                                    "InformationToGuest": {
                                        "required": true
                                    },
                                    "PastTrips": {
                                        "required": true
                                    },
                                    "PlannedTrips": {
                                        "required": true
                                    }
                                }
                            }
                        }
                    },
                    "messengers": {
                        "type": "object",
                        "required": true,
                        "additionalProperties": false,
                        "properties": {
                            "GOOGLE": {
                                "required": true
                            },
                            "SKYPE": {
                                "required": true
                            },
                            "Others": {
                                "required": true
                            }
                        }
                    },
                    "age": {
                        "type": "integer",
                        "required": true
                    },
                    "avatar": {
                        "type": "string",
                        "pattern": "^/members/avatar/.*$",
                        "required": true
                    },
                    "address": {
                        "type": "object",
                        "required": false,
                        "additionalProperties": false,
                        "properties": {
                            "@type": {
                                "pattern": "^Address$",
                                "required": false
                            },
                            "@id": {
                                "required": false
                            },
                            "houseNumber": {
                                "required": true
                            },
                            "streetName": {
                                "required": true
                            },
                            "zip": {
                                "required": true
                            },
                            "location": {
                                "type": "object",
                                "required": true,
                                "additionalProperties": false,
                                "properties": {
                                    "@type": {
                                        "pattern": "^Location$",
                                        "required": false
                                    },
                                    "@id": {
                                        "required": false
                                    },
                                    "name": {
                                        "required": true
                                    },
                                    "latitude": {
                                        "required": true
                                    },
                                    "longitude": {
                                        "required": true
                                    },
                                    "geonameId": {
                                        "required": true,
                                        "type": "integer"
                                    },
                                    "country": {
                                        "type": "object",
                                        "required": true,
                                        "additionalProperties": false,
                                        "properties": {
                                            "@type": {
                                                "pattern": "^Country$",
                                                "required": false
                                            },
                                            "@id": {
                                                "required": false
                                            },
                                            "geonameId": {
                                                "required": true,
                                                "type": "integer"
                                            },
                                            "name": {
                                                "required": true
                                            },
                                            "continent": {
                                                "required": true
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "homePhoneNumber": {
                        "type": "string",
                        "required": false
                    },
                    "cellPhoneNumber": {
                        "type": "string",
                        "required": false
                    },
                    "workPhoneNumber": {
                        "type": "string",
                        "required": false
                    }
                }
            }
        },
        "hydra:totalItems": {
            "type": "integer",
            "required": true
        },
        "hydra:view": {
            "type": "object",
            "required": true,
            "additionalProperties": false,
            "properties": {
                "@id": {"pattern": "^/api/members\\?city.country.name=.*$"},
                "@type": {"pattern": "^hydra:PartialCollectionView$"}
            }
        },
        "hydra:search": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "@type": {"pattern": "^hydra:IriTemplate$"},
                "hydra:template": {"pattern": "^/api/members{\\?username,username\\[\\],city.name,city.name\\[\\],city.country.name,city.country.name\\[\\],city.latitude,city.latitude\\[\\],city.longitude,city.longitude\\[\\],accommodation,accommodation\\[\\],maxguest,maxguest\\[\\],languageLevels.level,languageLevels.level\\[\\],languageLevels.language.name,languageLevels.language.name\\[\\],languageLevels.language.englishname,languageLevels.language.englishname\\[\\],languageLevels.language.shortcode,languageLevels.language.shortcode\\[\\]}$"},
                "hydra:variableRepresentation": {"pattern": "BasicRepresentation"},
                "hydra:mapping": {
                    "type": "array",
                    "additionalItems": false,
                    "maxItems": 22,
                    "minItems": 22,
                    "items": {
                        "@type": {"pattern": "^IriTemplateMapping$"},
                        "variable": {"pattern": "^username$"},
                        "property": {"pattern": "^username$"},
                        "required": {"type": "boolean"}
                    }
                }
            }
        }
    }
}
